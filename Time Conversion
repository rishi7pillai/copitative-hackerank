#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
#define ll long long int
#define mod 1e6+7
using namespace std;
int main()
{
    fast;string s;cin>>s;
    if(s[8]=='A')
    {
        if(s[0]=='1' and s[1]=='2')
           cout<<0<<0;
        else
            cout<<s[0]<<s[1];
    }
    else {
         if(s[0]=='1' and s[1]=='2')
            cout<<s[0]<<s[1];
         else {
            cout<<(10*abs(48-int(s[0])))+abs(48-int(s[1]))+12;
            // ll k=((10*(int(s[0])))+(int(s[1])));
            // cout<<k;
         } 
          
    }
    for(ll i=2;i<8;i++)
       cout<<s[i];
}
