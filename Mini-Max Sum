#include<bits/stdc++.h>
#define ll long long int
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
using namespace std;
int main(){
    fast;ll a[5];ll sum=0;
    for(ll i=0;i<5;i++) cin>>a[i];
    sort(a,a+5);
    for(ll i=0;i<4;i++) sum+=a[i];
    ll k=0; k=(sum+a[4])-a[0];
    cout<<sum<<" "<<k<<endl;
}
