#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define ll long long int   
#define endl "\n"
using namespace std;
int main(){
    fast;ll n;cin>>n;
    for(ll i=0;i<n;i++)
    {
        for(ll j=n-1;j>=0;j--)
        {
            if(i>=j)
              cout<<"#";
            else
              cout<<" ";
        }
        cout<<endl;
    }
}
